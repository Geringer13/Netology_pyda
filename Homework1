# задание 1
for i in range(1,101):
   if (i%3)==0 and (i%5)==0:
       print('FizzBuzz')
   elif (i%3)==0:
       print('Fizz')
   elif (i%5)==0:
       print('Buzz')
   else:
       print(i)




# задание 3
word = input('Введите слово ')
if len(word)%2==0:
   print('t')
elif len(word)%2!=0:
   print('es')
print('Конец программы')
